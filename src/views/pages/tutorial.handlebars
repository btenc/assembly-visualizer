<section id="tutorial">
  <h1>ASM Vis Documentation</h1>

  <h2>Available Commands</h2>
  <table>
    <thead>
      <tr>
        <th>Command</th>
        <th>Description</th>
        <th>Parameters</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>MOV</td>
        <td>Moves a value into a register.</td>
        <td><code>destination_register, source_value_or_register</code></td>
      </tr>
      <tr>
        <td>ADD</td>
        <td>Adds values from registers or constants.</td>
        <td><code>destination_register, source_value_or_register</code></td>
      </tr>
      <tr>
        <td>MUL</td>
        <td>Multiplies values from registers or constants.</td>
        <td><code>destination_register, source_value_or_register</code></td>
      </tr>
      <tr>
        <td>DIV</td>
        <td>Divides values, storing the result in the destination register and the remainder in the REM register.</td>
        <td><code>destination_register, source_value_or_register</code></td>
      </tr>
      <tr>
        <td>INC</td>
        <td>Increments the value in a register by 1.</td>
        <td><code>register</code></td>
      </tr>
      <tr>
        <td>DEC</td>
        <td>Decrements the value in a register by 1.</td>
        <td><code>register</code></td>
      </tr>
      <tr>
        <td>MOD</td>
        <td>Calculates the modulus of two values.</td>
        <td><code>destination_register, source_value_or_register</code></td>
      </tr>
      <tr>
        <td>NEG</td>
        <td>Negates the value in a register.</td>
        <td><code>register</code></td>
      </tr>
      <tr>
        <td>AND</td>
        <td>Performs a bitwise AND operation between two values.</td>
        <td><code>destination_register, source_value_or_register</code></td>
      </tr>
      <tr>
        <td>OR</td>
        <td>Performs a bitwise OR operation between two values.</td>
        <td><code>destination_register, source_value_or_register</code></td>
      </tr>
      <tr>
        <td>XOR</td>
        <td>Performs a bitwise XOR operation between two values.</td>
        <td><code>destination_register, source_value_or_register</code></td>
      </tr>
      <tr>
        <td>NOT</td>
        <td>Performs a bitwise NOT operation on a value.</td>
        <td><code>register</code></td>
      </tr>
    </tbody>
  </table>

