{{! reusable navbar element }}
<div class="navbar">
  <ul class="nav-ul">
    <li class="nav-left"><a href="/homepage">ASM Vis</a></li>
    <!-- if user logged in-->
    {{#if isLoggedIn}}
      <li class="nav-li"><a href="/snippets">New Snippet</a></li>
      <li class="nav-li-right"><a href="/users/logout">Log Out</a></li>
      <li class="nav-li-right"><a href="/users/{{username}}">Hello,
          {{username}}</a></li>
    {{else}}
      <!-- if user not logged in-->
      <li class="nav-li-right"><a href="/users/signup">Sign Up</a></li>
      <li class="nav-li-right"><a href="/users/login">Log In</a></li>
    {{/if}}
    <li class="nav-li"><a href="/snippets/tutorial">Tutorials</a></li>
  </ul>
</div>